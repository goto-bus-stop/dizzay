{
  "name": "dizzay",
  "description": "Simple CLI for plug.dj.",
  "version": "0.8.0",
  "license": "MIT",
  "keywords": [
    "plug",
    "plug.dj",
    "vlc",
    "music",
    "mplayer"
  ],
  "bugs": "https://github.com/goto-bus-stop/dizzay/issues",
  "author": "goto-bus-stop <rene@kooi.me>",
  "repository": {
    "type": "git",
    "url": "http://github.com/goto-bus-stop/dizzay.git"
  },
  "main": "lib/vlc-player",
  "bin": "bin/dizzay",
  "dependencies": {
    "blessed": "^0.1.81",
    "commander": "^2.9.0",
    "concat-stream": "^1.6.0",
    "debug": "^2.6.0",
    "http-proxy": "^1.16.2",
    "jsonist": "^2.1.0",
    "miniplug": "^1.3.3",
    "node-emoji": "^1.5.1",
    "pty.js": "^0.3.1",
    "run-series": "^1.1.4",
    "term.js": "0.0.7",
    "vlc-command": "^1.1.1",
    "ytdl-core": "^0.14.0"
  },
  "devDependencies": {
    "buble": "^0.15.2"
  },
  "scripts": {
    "prepublish": "npm run build",
    "install:ui": "cd dizzay-ui && npm install && npm link ../ && cd ../ && npm link ./dizzay-ui",
    "build:ui": "buble dizzay-ui/src -o dizzay-ui/lib --no modules",
    "build:main": "buble src -o lib --no modules",
    "build": "npm run build:main && npm run build:ui",
    "test": "echo 'No test specified' && true"
  }
}
